{
  "manifest_version": 3,
  "name": "Shorts Remover",
  "version": "0.0.8",
  "description": "Removes all shorts from youtube",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.min.js"
      ]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
